<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pidal.alfonso.w4group1user" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="pidal.alfonso.w4group1user.OfficeListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pidal.alfonso.w4group1user.OfficeDetailActivity"
            android:label="@string/title_office_detail"
            android:parentActivityName="pidal.alfonso.w4group1user.OfficeListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OfficeListActivity" />
        </activity>
    </application>

</manifest>
